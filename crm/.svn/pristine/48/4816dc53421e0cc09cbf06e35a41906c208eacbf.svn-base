<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zking.crm.base.entity.CstActivity" table="cst_activity">
		<id name="atvId" type="java.lang.Integer" column="atv_id">
			<generator class="native"></generator>
		</id>
		<property name="atvCustNo" type="java.lang.String"  column="atv_cust_no" />
		<property name="atvCustName" type="java.lang.String"  column="atv_cust_name" />
		<property name="atvDate" type="java.sql.Timestamp;"  column="atv_date" />
		<property name="atvPlace" type="java.lang.String"  column="atv_place" />
		<property name="atvTitle" type="java.lang.String"  column="atv_title" />
		<property name="atvRemark" type="java.lang.String"  column="atv_remark" />
		<property name="atvDesc" type="java.lang.String"  column="atv_desc" />
	</class>
</hibernate-mapping>
