package com.zking.crm.base.entity;

import java.io.Serializable;
import java.sql.Timestamp;

 /**
 * Sal_chance 营销管理:营销机会表
 * 2019-07-28 19:12:15
 */ 


public class SalChance implements Serializable {
	private Integer chcId;//营销机会ID:主键 标识列
